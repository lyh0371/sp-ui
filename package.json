{
  "name": "sp-ui-code",
  "version": "1.0.0",
  "description": "市面上基于 vue3 的组件库已经很泛滥，而`SP-UI` 的问世不是为了做一个完整的组件库，而是对其他组件库的一种补充，`SP-UI`会去思考些更加有趣且好用的组件",
  "main": "index.js",
  "scripts": {
    "w": "web-norm",
    "prepare": "husky install",
    "d": "gulp --require @esbuild-kit/cjs-loader -f ./scripts/buildVite/gulpfile.ts",
    "dev": "vite",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "dev:play": "pnpm -C play dev",
    "build:play": "pnpm -C play build",
    "build:full": "vite build",
    "copyTypes": "esno ./scripts/build/copyTypes",
    "move": "esno ./scripts/buildVite/remove",
    "build:types": "rimraf dist/types && esno ./scripts/build/buildTypes",
    "build:css": "vite build --config scripts/buildVite/vite.config.css.ts",
    "build:component": "vite build --config scripts/buildVite/vite.config.es.ts",
    "build:lib": "vite build --config scripts/buildVite/vite.config.lib.ts",
    "build": "pnpm run clean:dist && pnpm run build:css && pnpm run build:component && pnpm run build:types && pnpm run copyTypes",
    "clean:dist": "rimraf dist",
    "pre-commit": "lint-staged",
    "preinstall": "npx only-allow pnpm",
    "postinstallmac": "git config core.hooksPath .husky && chmod 700 .husky/*",
    "eslint": "eslint --cache --max-warnings 0  \"packages/**/*.{vue,ts,js,tsx}\" --fix",
    "commit": "git add . && git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyh0371/sp-ui.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lyh0371/sp-ui/issues"
  },
  "homepage": "https://sp-ui.vercel.app/",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-angular": "^17.0.3",
    "@commitlint/cz-commitlint": "^17.0.3",
    "@ctrl/tinycolor": "^3.4.1",
    "@esbuild-kit/cjs-loader": "^2.3.3",
    "@pnpm/types": "^8.5.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@sp-ui/components": "workspace:^1.0.0",
    "@types/escape-html": "^1.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-clean-css": "^4.3.0",
    "@types/gulp-rename": "^2.0.1",
    "@types/gulp-sass": "^5.0.0",
    "@types/prismjs": "^1.26.0",
    "@types/replacestream": "^4.0.1",
    "@types/rollup": "^0.54.0",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "chalk": "^5.0.1",
    "commitizen": "^4.2.4",
    "consola": "^2.15.3",
    "cz-customizable": "^6.9.0",
    "escape-html": "^1.0.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.2.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.1.0",
    "husky": "^8.0.1",
    "inquirer": "^8.0.0",
    "istextorbinary": "^6.0.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.7.1",
    "prismjs": "^1.29.0",
    "replacestream": "^4.0.3",
    "rollup": "^2.78.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-esbuild": "^4.9.3",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.54.5",
    "terser": "^5.14.2",
    "ts-morph": "^15.1.0",
    "unplugin-vue-define-options": "^0.6.1",
    "unplugin-vue-setup-extend-plus": "^0.3.2",
    "vite": "^3.0.8",
    "vite-plugin-dts": "^1.5.0",
    "vitepress-theme-demoblock": "^1.4.2",
    "w": "^1.1.0",
    "web-norm": "^1.0.24"
  },
  "lint-staged": {
    "*.{js,ts,vue,jsx,tsx}": [
      "npm run eslint"
    ],
    "*.{js,jsx,ts,tsx,md,html,css,lees,scss,sass}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@sp-ui/styles": "workspace:^1.0.0",
    "@sp-ui/utils": "workspace:^1.0.0",
    "vitepress": "1.0.0-alpha.8",
    "vue": "^3.2.37"
  }
}
